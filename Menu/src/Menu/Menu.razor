@namespace Skclusive.Material.Menu
@inherits MaterialComponent

 <Popover
    Class="@_Class"
    Style="@_Style"
    RootRef="@RootRef"
    DisablePortal="@DisablePortal"
    PortalTargetBodyRef="@AnchorRef"
    Open="@Open"
    OnClose="@HandleClose"
    AnchorRef="@AnchorRef"
    Elevation="@Elevation"
    ContentAnchorRef="@ContentAnchorRef"
    AnchorVerticalOrigin="@AnchorVerticalOrigin"
    AnchorHorizontalOrigin="@AnchorHorizontalOrigin"
    TransformVerticalOrigin="@TransformVerticalOrigin"
    TransformHorizontalOrigin="@TransformHorizontalOrigin"
    TransformHorizontalOriginValue="@TransformHorizontalOriginValue"
    TransformVerticalOriginValue="@TransformVerticalOriginValue"
    AnchorLeft="@AnchorLeft"
    AnchorTop="@AnchorTop"
    AnchorType="@AnchorType"
    TransitionDuration="@TransitionDuration"
    AppearTimeout="@AppearTimeout"
    EnterTimeout="@EnterTimeout"
    ExitTimeout="@ExitTimeout"
    MountOnEnter="@MountOnEnter"
    UnmountOnExit="@UnmountOnExit"
    OnEnter="@HandleEnterAsync"
    OnExited="@HandleExitedAsync"
    OnEntering="@HandleEnteringAsync"
    OnEntered="@HandleEnteredAsync"
    OnExit="@HandleExitAsync"
    OnExiting="@HandleExitingAsync"
    PaperStyle="@_PaperStyle"
    PaperClass="@_PaperClass"
    @attributes="Attributes">
    <MenuList
        Class="@_ListClass"
        Style="@_ListStyle"
        RootRef="@ListRef"
        Open="@Open"
        Variant="@Variant"
        Dense="@Dense"
        DisablePadding="@DisablePadding"
        AutoFocus="@ListAutoFocus"
        AutoFocusItem="@AutoFocusItem"
        DisableListWrap="@DisableListWrap"
        ContentAnchorRef="@ContentAnchorRef"
        OnKeyDown="@HandleKeyDownAsync">
        @ChildContent
    </MenuList>
</Popover>