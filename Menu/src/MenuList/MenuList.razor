@namespace Skclusive.Material.Menu
@inherits MaterialComponent

<CascadingValue
    Value="MenuContext">
    <List
        Role="menu"
        Class="@_Class"
        Style="@_Style"
        RootRef="@RootRef"
        Dense="@Dense"
        DisablePadding="@DisablePadding"
        TabIndex="@(AutoFocus ? 0 : -1)"
        OnKeyDown="@HandleKeyDownAsync"
        @attributes="Attributes">
        @ChildContent
    </List>
</CascadingValue>
