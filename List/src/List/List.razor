@namespace Skclusive.Material.List
@inherits MaterialComponent

<CascadingValue
    Value="ListContext">
    <Component
        RootRef="@RootRef"
        Tag="@Component"
        Class="@_Class"
        Style="@_Style"
        Role="@Role"
        TabIndex="@TabIndex"
        OnClick="@HandleClick"
        OnClickStop="@OnClickStop"
        OnClickPrevent="@OnClickPrevent"
        OnKeyDown="@HandleKeyDownAsync"
        @attributes="Attributes">
        @SubheaderContent
        @ChildContent
    </Component>
</CascadingValue>