@namespace Skclusive.Material.Link
@inherits MaterialComponent

<Typography
    RootRef="@RootRef"
    Component="@Component"
    Color="@Color"
    Align="@Align"
    Display="@Display"
    GutterBottom="@GutterBottom"
    NoWrap="@NoWrap"
    Paragraph="@Paragraph"
    Variant="@_Variant"
    Class="@_Class"
    Style="@_Style"
    href="@Href"
    OnClick="@HandleClickAsync"
    OnClickStop="@OnClickStop"
    OnClickPrevent="@OnClickPrevent"
    @attributes="Attributes">
    @ChildContent
</Typography>
