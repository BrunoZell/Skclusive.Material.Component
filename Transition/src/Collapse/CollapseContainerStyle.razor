@namespace Skclusive.Material.Transition
@inherits StyleComponentBase

.CollapseContainer-Root {
    height: 0;
    overflow: hidden;
    transition: height 300ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
}

.CollapseContainer-Entered {
    height: auto;
    overflow: visible;
}

.CollapseContainer-Hidden {
    visibility: hidden;
}

.CollapseContainer-Wrapper {
    display: flex;
}

.CollapseContainer-WrapperInner {
    width: 100%;
}