@namespace Skclusive.Material.Popover
@inherits StyleComponentBase

.Popover-Root {
}

.Popover-Paper {
    position: absolute;
    overflow-y: auto;
    overflow-x: hidden;
    min-width: 16px;
    min-height: 16px;
    max-width: calc(100% - 32px);
    max-height: calc(100% - 32px);
    outline: 0px;
}