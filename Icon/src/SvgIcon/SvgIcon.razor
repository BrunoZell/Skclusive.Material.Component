@namespace Skclusive.Material.Icon
@inherits SvgIconBase

<svg
    class="@_Class"
    style="@_Style"
    focusable="false"
    viewBox="@ViewBox"
    color="@HtmlColor"
    role="@_Role"
    aria-hidden="@Hidden"
    @onclick="@HandleClickAsync"
    @onclick:preventDefault="@PreventDefault"
    @onclick:stopPropagation="@StopPropagation"
    @attributes="Attributes">
    @if (!Hidden)
    {
        <title>@Title</title>
    }
    @ChildContent
</svg>